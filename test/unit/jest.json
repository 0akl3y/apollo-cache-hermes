{
  "automock": false,
  "collectCoverageFrom": [
    "src/**/*.js"
  ],
  "coveragePathIgnorePatterns": [
    "/node_modules/",
    "<rootDir>/test/"
  ],
  "mapCoverage": true,
  "rootDir": "../..",
  "setupTestFrameworkScriptFile": "./test/env/unit.js",
  "testMatch": ["<rootDir>/test/unit/**/*.js"]
}
